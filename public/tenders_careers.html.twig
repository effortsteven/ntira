<!-- tenders modal -->
<div class='tender_modal'>
</div>

<!-- tenders modal -->
<div class="career_modal">
</div>

</body>
</html>

{% include "common/script_links.html.twig" %}

<script type="text/javascript">

    $(document).ready(function(){

        // $(".tenders").html(tenders());
        // $(".tender_count").html(tenders().length);
        // $(".careers").html(careers());
        // $(".career_count").html(careers().length);

        $('.bottom-div').hide();

        $('#apply_form').click(function(){
            $('.bottom-div').slideDown(1500).show();
        });

        $('#cancel_form').click(function(){
            $('.bottom-div').slideUp(1500);
            $.notify({
                // options
                message: 'Application process has been canceled!'
            },{
                // settings
                type: 'danger',
                mouse_over: 'pause'
            });
        });


        $('#send_form').click(function(){
            //var ip_address = url+"nic_website/jobApplication/";
            var ip_address = url+"jobApplication/";
            var university_list = [];
            var school_list = [];
            $(".uni_div").each(function(){
                var university = $(this).find("input[name='university']").val();
                console.log(university);
                var university_finish_year = $(this).find("input[name='university_finish_year']").val();
                var univesity_course = $(this).find("input[name='univesity_course']").val();
                var university_pass = $(this).find("input[name='university_pass']").val();

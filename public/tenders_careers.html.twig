        function share(){

            var ul,uu,temp_holder;

            uu = window.location.href;

            var temp_holder = document.createElement('input'),

                text = uu;

            document.body.appendChild(temp_holder);
            temp_holder.value = text;
            temp_holder.select();
            document.execCommand('copy');
